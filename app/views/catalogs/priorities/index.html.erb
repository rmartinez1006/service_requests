<h1>Catálogo de prioridades</h1>

<table class ="table1">
  <tr class="table_header">
    <th>ABREV.</th>
    <th>DESCRIPCION</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @catalogs_priorities.each do |catalogs_priority| %>
  <tr>
    <td><%= catalogs_priority.abbr %></td>
    <td><%= catalogs_priority.description %></td>
    <td><%= link_to 'Ver', catalogs_priority %></td>
    <td><%= link_to 'Modificar', edit_catalogs_priority_path(catalogs_priority) %></td>
    <td><%= link_to 'Eliminar', catalogs_priority, :confirm => 'Confirmar eliminación.', :method => :delete %></td>
  </tr>
<% end %>
</table>

<hr />

