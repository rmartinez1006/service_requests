<h1>Presupuestos</h1>

<table class="table1">
  <tr>
    <th>Solicitud</th>
    <th>Costo Total</th>
    <th>Fecha</th>
    <th>Responsable</th>
    <th></th>    
    <th></th>
  </tr>

<% @budgets_budgets.each do |budgets_budget| %>
  <tr>
    <td><%= budgets_budget.request.num_sequence %></td>
    <td><%= budgets_budget.total_cost %></td>
    <td><%= budgets_budget.budget_date %></td>
    <td><%= budgets_budget.user_id %></td>    
    <td><%= link_to 'Editar', :controller=>'/budgets/budgets', :action => "budget_fm1",  :id => budgets_budget.support_request_id %></td>
    <td><%= link_to 'Borrar', budgets_budget,:remote => true, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
